{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "main": "./server/bin/www",
  "description": "Newsfeed server to fetch news",
  "scripts": {
    "start": "nodemon ./server/bin/www",
    "client-install": "cd client && npm install",
    "server": "nodemon server.js",
    "client": "npm start --prefix client/",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "devDependencies": {
    "morgan": "~1.9.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.18.2",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "newsapi": "^2.4.0",
    "request": "^2.87.0",
    "snyk": "^1.97.1"
  },
  "snyk": true
}
